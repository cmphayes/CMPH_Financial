@model IEnumerable<CMPH_Financial.Models.Account>

@{
    ViewBag.Title = "Index";
}

<h2>AcountPartial</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.HouseholdId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InitialBalance)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReconciledBalance)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.HouseholdId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InitialBalance)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReconciledBalance)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
            </td>
        </tr>
    }

</table>
